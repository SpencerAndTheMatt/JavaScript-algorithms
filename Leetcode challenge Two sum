/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    
    let arr = nums;
    let newArr = [];
    
    for (let i = 0; i < arr.length; i++) {
        for (let j = i+1; j < arr.length; j++) {
            if (arr[i] + arr[j] == target) {
                newArr.push(i);
                newArr.push(j);
            }
        }
    }
    
    return newArr;
    
};
//
